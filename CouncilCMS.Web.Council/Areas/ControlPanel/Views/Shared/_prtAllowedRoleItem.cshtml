@model AllowedRole

<div class='card-sm' id="itemItem@(Model.ItemId)Role@(Model.RoleId)">
    <div class='row'>
        <div class="col-sm-10">
            @Model.Name
        </div>        
        <div class="col-sm-2">
            <span class='link-btn nav-link' data-action="remove-user" data-item-id="@Model.ItemId" data-user-id="@Model.RoleId">
                <i class='fa fa-fw fa-trash'></i>
            </span>
        </div>
    </div>
</div>